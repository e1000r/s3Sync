# Remote (AWS S3 or any S3-compatible remote)
REMOTE_BUCKET=my-remote-bucket
REMOTE_REGION=us-east-1
REMOTE_ACCESS_KEY=your-remote-access-key
REMOTE_SECRET_KEY=your-remote-secret-key
REMOTE_ENDPOINT=s3.amazonaws.com
REMOTE_USE_SSL=true

# Local (MinIO or other S3-compatible)
LOCAL_BUCKET=my-local-bucket
LOCAL_REGION=us-east-1
LOCAL_ACCESS_KEY=your-local-access-key
LOCAL_SECRET_KEY=your-local-secret-key
LOCAL_ENDPOINT=minio:9000
REMOTE_USE_SSL=false
